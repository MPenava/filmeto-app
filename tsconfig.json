{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@api/*": [
        "./api/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@features/*": [
        "./features/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
    }
  },
  "include": [
    ".eslintrc.cjs",
    "babel.config.js",
    "openapi-ts.config.ts",
    "src",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
