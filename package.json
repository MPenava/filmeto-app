{
  "name": "filmeto-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "knip": "knip",
    "gen-api": "openapi-ts -i ./src/api/specs/openapi.yaml -o ./src/api/services --client axios --lint eslint",
    "gen-mocks": "npx msw-auto-mock ./src/api/specs/openapi.yaml -o ./src/api/mocks/generated/handlers.ts --static --base-url='http://localhost:4000/v1' --static",
    "gen-all": "pnpm gen-api && pnpm gen-mocks",
    "check": "pnpm tsc && pnpm lint && pnpm knip"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@faker-js/faker": "^9.7.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.29.2",
    "axios": "^1.6.8",
    "expo": "~52.0.35",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.6",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "fast-text-encoding": "^1.0.6",
    "i18next": "^24.0.2",
    "knip": "^5.50.5",
    "msw-auto-mock": "^0.31.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.1.2",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@hey-api/openapi-ts": "0.42.1",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "msw": "^2.7.5",
    "openapi-typescript-codegen": "^0.29.0",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
